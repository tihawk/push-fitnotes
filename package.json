{
  "name": "push-fitnotes",
  "productName": "push-fitnotes",
  "version": "0.4.0",
  "description": "An app to convert FitNotes exported workouts into .fit binaries, and upload them to Garmin Connect.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [
    "electron",
    "fitnotes",
    "fit",
    "garmin",
    "workout",
    "upload",
    "fitness"
  ],
  "author": {
    "name": "Kiril Panayotov Blagoev",
    "email": "blagoeffkp@gmail.com"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css,jsx,ts,tsx}": [
      "prettier --trailing-comma es5 --tab-width 2 --bracket-spacing --no-semi --single-quote --write",
      "git add"
    ],
    "*.{js,css,md}": "prettier --write"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@electron-forge/plugin-webpack": "^6.0.5",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "electron": "23.0.0",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.27.5",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "lint-staged": "^13.1.1",
    "node-loader": "^2.0.0",
    "prettier": "^2.8.4",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@types/electron-json-storage": "^4.5.0",
    "csv": "^6.2.7",
    "electron-json-storage": "^4.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "fit-encoder": "^0.1.5",
    "garmin-connect": "^1.4.5",
    "husky": "^4.3.8",
    "material-icons": "^1.13.1",
    "materialize-css": "^1.0.0",
    "sweetalert2": "^11.7.1"
  }
}
